# Dashboard Test Environment Configuration

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:13000

# Userfront Configuration (Test Mode)
NEXT_PUBLIC_USERFRONT_WORKSPACE_ID=test_workspace_id

# Application Environment
NODE_ENV=test
NEXT_TELEMETRY_DISABLED=1

# Testing Configuration
JEST_JUNIT_OUTPUT_DIR=./coverage
JEST_JUNIT_OUTPUT_NAME=junit.xml

# Feature Flags for Testing
ENABLE_ANALYTICS=false
ENABLE_ERROR_REPORTING=false
ENABLE_PERFORMANCE_MONITORING=false

# Mock/Test Data
USE_MOCK_DATA=true
MOCK_USER_AUTHENTICATED=true
MOCK_TENANT_ID=test-tenant-123

# Accessibility Testing
A11Y_TESTING_ENABLED=true
PERFORMANCE_TESTING_ENABLED=true

# Development/Debug Settings  
DEBUG_NAVIGATION=false
DEBUG_AUTHENTICATION=false
DEBUG_API_CALLS=false